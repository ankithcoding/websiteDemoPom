<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
	"http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>

	<definition name="contact" extends="default">
		<put-attribute name="title" type="string" value="Contact Us" />
		<put-attribute name="id" type="string" value="contactus" />
		<put-attribute name="body">
			<definition extends="twoColumn">
				<put-list-attribute name="columnOne">
					<add-attribute value="/WEB-INF/views/contactus/contactForm.jsp" />
				</put-list-attribute>
				<put-list-attribute name="columnTwo">
					<add-attribute value="/WEB-INF/views/contactus/branches.jsp" />
				</put-list-attribute>
			</definition>
		</put-attribute>
	</definition>

	<definition name="contact/success" extends="default">
		<put-attribute name="title" type="string" value="Contact Us" />
		<put-attribute name="id" type="string" value="contactus" />
		<put-attribute name="body">
			<definition extends="twoColumn">
				<put-list-attribute name="columnOne">
					<add-attribute value="/WEB-INF/views/contactus/success.jsp" />
				</put-list-attribute>
				<put-list-attribute name="columnTwo">
					<add-attribute value="/WEB-INF/views/contactus/branches.jsp" />
				</put-list-attribute>
			</definition>
		</put-attribute>
	</definition>
	
</tiles-definitions>